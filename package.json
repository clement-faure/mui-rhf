{
  "name": "mui-rhf",
  "version": "0.0.11",
  "description": "Form generator based on Material UI and React Hook Form",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "clement-faure <dev@clement-faure.fr>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clement-faure/mui-rhf.git"
  },
  "keywords": [
    "react",
    "typescript",
    "npm",
    "material-ui",
    "react-hook-form"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "test:unit:dev": "jest --watch",
    "test:unit": "jest",
    "coverage:unit": "jest --coverage",
    "coverage:unit:open": "yarn coverage:unit; open coverage/lcov-report/index.html",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "tsc -w"
  },
  "files": [
    "dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/clement-faure/mui-rhf/issues"
  },
  "homepage": "https://github.com/clement-faure/mui-rhf#readme",
  "dependencies": {},
  "peerDependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.8.6"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "auto-changelog": "^2.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "prettierrc": "^0.0.0-5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^7.8.6",
    "ts-jest": "^27.0.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2"
  }
}