{
  "name": "mui-rhf",
  "version": "0.0.4",
  "description": "Form generator based on Material UI and React Hook Form",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "clement-faure <dev@clement-faure.fr>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clement-faure/mui-rhf.git"
  },
  "keywords": [
    "react",
    "typescript",
    "npm",
    "material-ui",
    "react-hook-form"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "test:unit:dev": "jest --watch",
    "test:unit": "jest",
    "coverage:unit": "jest --coverage; open coverage/lcov-report/index.html",
    "build": "cross-env NODE_ENV=production tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "tsc -w"
  },
  "files": [
    "dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/clement-faure/mui-rhf/issues"
  },
  "homepage": "https://github.com/clement-faure/mui-rhf#readme",
  "dependencies": {},
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.2"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "auto-changelog": "^2.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettierrc": "^0.0.0-5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.2",
    "ts-jest": "^26.5.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}